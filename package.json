{
  "name": "alitacodechat",
  "displayName": "AlitaCodeChat",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "alitacode",
          "title": "AlitaCode",
          "icon": "static/icon.png"
        }
      ]
    },
    "views": {
      "alitacode": [
        {
          "type": "webview",
          "id": "alitacodechat.view",
          "name": "Chat"
        }
      ]
    },
    "commands": [
      {
        "command": "alitacodechat.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "html-modifier": "^1.2.0"
  }
}
